generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
  admin Boolean @default(false)

  @@map("users")
}

model BroadcastersReocord {
  id              Int    @id @default(autoincrement())
  broadcasterName String @unique
  city            String
  state           String
  codec           String
  emails          String @db.LongText
}

model BroadcastersBand {
  id              Int    @id @default(autoincrement())
  broadcasterName String @unique
  city            String
  state           String
  codec           String
  emails          String @db.LongText
}

model recordHistory {
  id           Int      @id @default(autoincrement())
  date         DateTime
  destinations String
  clock        String
  user         String
}
